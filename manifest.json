{
	"manifest_version": 2,
	"name": "Imager",
	"version": "1.3.0",
	"permissions": ["menus", "contextMenus", "tabs", "<all_urls>", "storage"],
	"background": {
		"scripts": ["imager.js"]
	},
	"icons": {
		"16": "icons/icon_16.png",
		"32": "icons/icon_32.png",
		"48": "icons/icon_48.png",
		"64": "icons/icon_64.png",
		"128": "icons/icon_128.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"]
		}
	],
	"options_ui": {
		"page": "options.html"
	}
}
