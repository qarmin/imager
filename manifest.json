{
  "manifest_version": 2,
  "name": "Imager",
  "version": "1.0",
  "permissions": [
    "identity",
    "menus",
    "contextMenus",
    "downloads",
    "storage",
    "tabs",
    "<all_urls>"
  ],
  "background": {
    "scripts": [
      "imager.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}